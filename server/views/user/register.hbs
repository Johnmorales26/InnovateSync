<div>
  <h1 class='hedaer center'>Registro</h1>
</div>

<div class='row'>
  <form action='/user/register' method='post' class='col s12 card z-depth-3'>
    <div class='row'>
      <div class='input-field col s12 m6'>
        <i class='material-icons prefix'>
          account_circle
        </i>
        <input name='fullname' id='fullname' type='text' class='validate' />
        <label for='fullname'>
          Nombre Completo
        </label>
      </div>
    </div>
    <div class='row'>
      <div class='input-field col s12 m6'>
        <i class='material-icons prefix'>
          code
        </i>
        <input
          name='studentCode'
          id='studentCode'
          type='number'
          class='validate'
        />
        <label for='studentCode'>
          Codigo de Estudiante
        </label>
      </div>
    </div>
    <div class='row'>
      <div class='input-field col s12 m6'>
        <i class='material-icons prefix'>
          gps_fixed
        </i>
        <input name='grade' id='grade' type='number' class='validate' />
        <label for='grade'>
          Grado
        </label>
      </div>
      <div class='input-field col s12 m6'>
        <i class='material-icons prefix'>
          location_on
        </i>
        <input name='section' id='section' type='text' class='validate' />
        <label for='section'>
          Secci√≥n
        </label>
      </div>
    </div>
    <div class='row center-align'>
      <button type='submit' class='waves-effect waves-light btn'>
        <i class='material-icons left'>send</i>Registrar
      </button>
    </div>
  </form>
</div>